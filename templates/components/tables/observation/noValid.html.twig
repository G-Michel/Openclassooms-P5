<div class="tab-pane fade {% if active|default(false) %}show active{% endif %}" id="{{ id }}Obs" role="tabpanel" aria-labelledby="{{ id }}-tab">
    <table class="table table-striped table-light mt-3">
      <thead>
        <tr>
          <th scope="col">ID Observation</th>
          <th scope="col">Lieu</th>
          <th scope="col">Moment</th>
          <th scope="col">Oiseau</th>
          <th scope="col">Raison</th>
          <th scope="col-action"></th>
        </tr>
      </thead>
      <tbody>
        {% if id == 'uncommitted'%}
          {% for obs in observations %}
            {{ include('components/tables/observation/_table_noValid.html.twig')}}
          {% endfor %}
        {% else %}
          {% for obs in observations if obs.status <= -199  %}
            {{ include('components/tables/observation/_table_default.html.twig')}}
          {% endfor %}
        {% endif %}
      </tbody>
    </table>
  </div>